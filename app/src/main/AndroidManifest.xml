<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xdroid.rxjava">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".application.RxJavaApplication"
        android:allowBackup="true"
        android:icon="@mipmap/launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.RxDemoActivity" />
        <activity android:name=".activity.RxActivity" />
        <activity android:name=".activity.RxOp1Activity" />
        <activity android:name=".activity.RxOp2Activity" />
        <activity android:name=".activity.RxOp3Activity" />

        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>